<form name="formBorrar" method="post" action="{{ path('cliente_delete', {'id': cliente.id}) }}">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cliente.id) }}">
    <button class="btn btn-danger btn-block" onclick="pregunta(event);"><i class="fa fa-minus "></i> Delete</button>
</form>