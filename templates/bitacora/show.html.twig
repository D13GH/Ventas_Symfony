{% extends 'base.html.twig' %}

{% block title %}Bitacora{% endblock %}

{% block body %}
    <h1>Bitacora</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ bitacora.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ bitacora.email }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ bitacora.token }}</td>
            </tr>
            <tr>
                <th>FecIni</th>
                <td>{{ bitacora.fecIni ? bitacora.fecIni|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FecFin</th>
                <td>{{ bitacora.fecFin ? bitacora.fecFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('bitacora_index') }}">back to list</a>

    <a href="{{ path('bitacora_edit', {'id': bitacora.id}) }}">edit</a>

    {{ include('bitacora/_delete_form.html.twig') }}
{% endblock %}